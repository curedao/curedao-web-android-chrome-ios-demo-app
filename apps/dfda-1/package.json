{
  "name": "qm-api",
  "version": "1.0.0",
  "scripts": {
    "express:start": "cd public/app && npm run start",
    "cy:open": "cypress open",
    "cy:open:staging": "cross-env-shell APP_URL=https://staging.quantimo.do cypress open",
    "cy:open:feature": "cross-env-shell APP_URL=https://feature.quantimo.do cypress open",
    "cy:open:local": "cross-env-shell APP_URL=https://local.quantimo.do cypress open",
    "cy:run": "cypress run",
    "cy:run:feature": "cross-env-shell APP_URL=https://feature.quantimo.do cypress run",
    "cy:install": "npm install -g cypress && cypress install",
    "cy:record": "cypress run --record --key 662e64b2-03c7-4d07-9fcb-25f72d37b257",
    "laravel:serve": "php artisan serve",
    "cy:serve:open": "cross-env-shell APP_ENV=localhost npm-run-all --parallel laravel:serve cy:open",
    "cy:serve:run": "cypress install && cross-env-shell APP_ENV=localhost npm-run-all --parallel laravel:serve cy:run",
    "dev": "mix",
    "hot": "mix watch --hot",
    "ionic:clone": "git clone https://github.com/mikepsinn/cd-ionic public/app || true && cd public/app && npm install",
    "ionic:serve": "cd public/app && npm run start & ",
    "mocha": "mocha public/app/tests/mocha/**/*.test.js",
    "postinstall": "cd public/app && npm install",
    "production": "mix --production",
    "watch": "mix watch",
    "watch-poll": "mix watch --watch-options-poll=1000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikepsinn/qm-api.git"
  },
  "author": "mikepsinn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikepsinn/qm-api/issues"
  },
  "homepage": "https://github.com/mikepsinn/qm-api#readme",
  "devDependencies": {
    "@bugsnag/js": "7.18.0",

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../your_db.sqlite"
}

model action_events {
  id              Int       @id @default(autoincrement())
  batch_id        String
  user_id         Int
  name            String
  actionable_type String
  actionable_id   Int
  target_type     String
  target_id       Int
  model_type      String
  model_id        Int?
  fields          String
  status          String    @default("running")
  exception       String
  created_at      DateTime?
  updated_at      DateTime?
  original        String?
  changes         String?

  @@index([user_id], map: "action_events_user_id_index")
  @@index([batch_id, model_type, model_id], map: "action_events_batch_id_model_type_model_id_index")
  @@index([actionable_type, actionable_id], map: "action_events_actionable_type_actionable_id_index")
}

model aggregate_correlations {
